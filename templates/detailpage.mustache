  {{!                                                                                                                                 
    @template block/ocmyproducts                                                                                                        

    detalpage of isymetaselect
                                                                                                                                    
    Classes required for JS:                                                                                                        
    * none                                                                                                                        
                                                                                                                                    
    Data attributes required for JS:                                                                                                
    * none                                                                                                                
                                                                                                                                    
    Context variables required for this template:
    * url
    * teasertext
    * videocode
    * altpic
    * learninggoals
    * gliederung
    * alllecturers
    * allauthors
    * detailsmorelecturer
    * targetgroup
    * certificate
    * license
    * taglist
    * tagurlarray

    * coursetitle
    * edit_string
    * lecturer
    * uni
    * language
    * subject
    * meta4
    * meta5
    * fileurl

    * is_enrolled
    * alllecturers_notempty
    * allauthors_notempty

    * lecturer_detail
    * meta2_detail
    * courselanguages_detail
    * meta6_detail
    * avgworkload_detail
    * hours_detail
    * meta5_detail

    * free
    * enrol
    * tocourse

    * h_awaits
    * h_learn
    * h_outline
    * h_further_auth
    * h_target_group
    * h_confirmation
    * license
                                                                                                                                    
  }}



		



  <div class="metadetail-container">
	<div class="metadetail-header">
		<div class="metadetail-header-image metatile-img" style="background-image:url('{{overviewimage}}');">
    {{^started}}
      <div class="icon-coming-soon"></div>
    {{/started}}
		</div>
		<div class="metadetail-header-infos">
			<h2>{{{coursetitle}}} {{{edit_string}}}</h2>
      <table class="metatile-table">
				<tr>
					<td><i class="icon fa fa-bullseye fa-fw"></i>{{meta1_name}}:</td>
					<td>{{meta1}}</td>
				</tr>
				<tr>
					<td><i class="icon fa fa-list-alt fa-fw"></i>{{meta2_name}}:</td>
					<td>{{{meta2}}}</td>
				</tr>
				<tr>
					<td><i class="icon fa fa-user-circle fa-fw"></i>{{meta3_name}}:</td>
					<td>{{{meta3}}}</td>
				</tr>
				<tr>
					<td><i class="icon fa fa-tasks fa-fw"></i>{{meta4_name}}:</td>
					<td>â‰ˆ {{{meta4}}} {{{hours}}}</td>
				</tr>
				<tr>
					<td><i class="icon fa fa-calendar-check-o fa-fw"></i>{{meta5_name}}:</td>
					<td>{{{meta5}}}</td>
				</tr>
				<tr>
					<td><i class="icon fa fa-cubes fa-fw"></i>{{{meta6_name}}}:</td>
					<td>{{{meta6}}}</td>
				</tr>
			</table>
		</div>
	</div>

	<div class="meta-share">
    <div class="meta-share-facebook"><a href="https://www.facebook.com/sharer/sharer.php?u={{{url}}}">{{# str}} metashare, block_isymetaselect {{/ str}}</a></div>
		<div class="meta-share-twitter"><a href="https://twitter.com/intent/tweet?url={{{url}}}">{{# str}} metatweet, block_isymetaselect {{/ str}}</a></div>
		<div class="meta-share-whatsapp"><a href="whatsapp://send?text={{{url}}}">{{# str}} metashare, block_isymetaselect {{/ str}}</a></div>
		<div class="meta-share-linkedin"><a href="https://www.linkedin.com/shareArticle?mini=true&amp;summary=&amp;url={{{url}}}">{{# str}} metashare, block_isymetaselect {{/ str}}</a></div>
		<div class="meta-share-xing"><a href="https://www.xing.com/social_plugins/share?url={{{url}}}">{{# str}} metashare, block_isymetaselect {{/ str}}</a></div>
		<div class="meta-share-mail"><a href="mailto:?subject={{coursetitle}}&amp;body={{{url}}}">{{# str}} metashare, block_isymetaselect {{/ str}}</a></div>
	</div>

	<div class="metadetail-body">

		<div class="md-teasertext">
    {{#teasertext}}
      <h2>{{{h_awaits}}}</h2>
      {{{teasertext}}}
    {{/teasertext}}
    </div>
		
    {{#videocode}}
      <div class="md-teaservideo">
     <nolink> {{{videocode}}}</nolink>
      </div>
    {{/videocode}}
    {{^videocode}}
    <div class="md-teaservideo {{emptyteaser}}">
      <img src="{{detailimage}}">
      </div>
    {{/videocode}}
    
    {{#learninggoals}}
		<div class="md-learninggoals">
      <h2>{{{h_learn}}}</h2>
      {{{learninggoals}}}
    </div>
    {{/learninggoals}}
    {{#gliederung}}
		<div class="md-structure">
      <h2>{{{h_outline}}}</h2>
      {{{gliederung}}}
    </div>
    {{/gliederung}}


    {{#alllecturers_notempty}}
		<div class="md-lecturer">
    <h2>{{{lecturer_detail}}}</h2>
    {{/alllecturers_notempty}}

    {{#alllecturers}}
      <div class="lecturer_container"><div class="lecturer_imgcontainer {{class}}"><img src="{{img}}"/></div><div class="lecturer_text">{{{div}}}</div></div>
    {{/alllecturers}}

    {{#alllecturers_notempty}}
    </div>
    {{/alllecturers_notempty}}


    {{#allauthors_notempty}}
    <div class="md-lecturer">
      <h2>{{{h_further_auth}}}</h2>
    {{/allauthors_notempty}}

    {{#allauthors}}
      <div class="lecturer_container"><div class="lecturer_imgcontainer {{class}}"><img src="{{img}}"/></div><div class="lecturer_text">{{{div}}}</div></div>
    {{/allauthors}}

{{#allauthors_notempty}}
    </div>
    {{/allauthors_notempty}}

    {{#detailsmorelecturer}}
		<div class="md-morelecturer">
      <h2>{{{h_further_auth}}}</h2>
      {{{detailsmorelecturer}}}
    </div>
    {{/detailsmorelecturer}}

    {{#targetgroup}}
		<div class="md-targetgroup">
      <h2>{{{h_target_group}}}</h2>
      {{{targetgroup}}}
    </div>
    {{/targetgroup}}

    {{#certificate}}
		<div class="md-certificateofachievement">
      <h2>{{{h_confirmation}}}</h2>
      {{{certificate}}}
    </div>
    {{/certificate}}

    {{#allsponsors_notempty}}
      
          <div class="md-sponsors">
          <h2>{{{h_sponsors}}}</h2>
          <div class="sponsor_container">
          {{#allsponsors}}
            
              <div class="sponsor_imgcontainer {{classspons}}">
                <a href="{{{divspons}}}"><img src="{{imgspons}}"/></a>
              </div>
            
          {{/allsponsors}}
          </div>
          </div>
        

    {{/allsponsors_notempty}}



    {{#license}}
		<div class="md-license">
      <h2>{{{licensetitle}}}</h2>
      {{{license}}}
    </div>
    {{/license}}

    
		<div class="md-tags">
{{#taglist}}      <span class="metatag"><a href="{{url}}">{{tag}}</a></span>{{/taglist}}
    </div>
    
	</div>
</div>